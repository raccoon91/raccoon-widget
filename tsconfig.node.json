{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@resources/*": [
        "resources/*"
      ],
      "@app/*": [
        "src/renderer/app/*"
      ],
    },
    "types": [
      "electron-vite/node"
    ]
  }
}